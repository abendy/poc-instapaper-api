{
    "name": "poc-instapaper-api",
    "description": "localhost server + UI for Instapaper API",
    "repositories": [{
        "type": "package",
        "package": {
            "name": "abendy/instapaperoauth",
            "type": "library",
            "version": "0.1",
            "source": {
                "url": "https://github.com/abendy/instapaperoauth.git",
                "type": "git",
                "reference": "development"
            },
            "autoload": {
                "psr-4": {
                    "Abendy\\InstapaperOauth\\": "src"
                }
            }
        }
    }],
    "require": {
        "abendy/instapaperoauth": "*",
        "twig/twig": "^2.6",
        "predis/predis": "^1.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "kint-php/kint": "^3.1",
        "kint-php/kint-js": "^2.0",
        "kint-php/kint-twig": "^2.0",
        "symfony/polyfill-mbstring": "^1.10",
        "symfony/polyfill-iconv": "^1.10",
        "heroku/heroku-buildpack-php": "^148.0"
    }
}
